<c:FragmentDefinition
	xmlns="sap.m"
	xmlns:c="sap.ui.core"
	xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core"
	xmlns:customData="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
>
	<Dialog
		id="openDialog"
		title="{i18n>registerOrder}"
	>

		<!-- <beginButton>
			<Button
				text="{i18n>save}"
				type="Accept"
				press="handleSaveBtnPress"
			/>
            
		</beginButton>
        	

        
		<endButton>
			<Button
			
				text="{i18n>cancel}"
				type="Reject"
				press="handleCancelBtnPress"
			/>
		</endButton> -->


		<f:SimpleForm
			id="RegisterForm"
			editable="true"
		>
			<f:content>
			
				<Table
					id="tableProducts"
					items="{servicesAndProducts>/Products}"
				>
					<headerToolbar>
						<Toolbar>
							<Button
								icon="sap-icon://add"
								text="Row"
								press="addRow"
							/>
							<!-- <Button
													icon="sap-icon://display"
													text="Row"
													press="fetchRecords"
												/> -->
						</Toolbar>
					</headerToolbar>
					<columns>
						<Column width="50px"/>
						<Column
							minScreenWidth="Tablet"
							demandPopin="true"
							width="280px"
						>
							<Text text="{i18n>description}"/>
						</Column>
						<Column width="130px">
							<Text text="{i18n>category}"/>
						</Column>
						<Column
							width="100px"
							minScreenWidth="Tablet"
							demandPopin="true"
						>
							<Text text="{i18n>quantity}"/>
						</Column>
						<Column
							minScreenWidth="Tablet"
							demandPopin="true"
							width="80px"
						>
							<Text text="{i18n>unit}"/>
						</Column>
						<Column
							minScreenWidth="Tablet"
							demandPopin="true"
							width="100px"
						>
							<Text text="{i18n>value}"/>
						</Column>
						<Column
							minScreenWidth="Tablet"
							demandPopin="true"
							width="100px"
						>
							<Text text="{i18n>total}"/>
						</Column>
					</columns>
					<items>
						<ColumnListItem>
							<cells>
								<Button
									icon="sap-icon://delete"
									press="deleteRow"
									type="Reject"
								/>
								<!-- <Input value="{Category}"/> -->
								<Input
									id="InputDescription"
									value="{servicesAndProducts>Description}"
									showClearIcon="true"
									required="true"
									showSuggestion="true"
									suggestionItems="{
										path: '/ZshLzSrvPrdSet',
										templateShareable: false
									}"
									change="_onChangeDescription"
								>
									<suggestionItems>
										<core:ListItem
											key="{Description}"
											text="{Description}"
											additionalText="{Value}"
										/>
									</suggestionItems>
								</Input>
								<ComboBox
									id="multiComboBox"
									customData:hasValue="true"
									value=""
									selectedKey="{servicesAndProducts>Category}"
								>
									<core:Item
										key="SRV"
										text="ServiÃ§o"
									/>
									<core:Item
										key="PRT"
										text="Produto"
									/>
								</ComboBox>
								<Input
									value="{servicesAndProducts>Quantity}"
									change="_onChangeQuantity"
									type="Number"
									showClearIcon="true"
									required="true"
								/>
								<Input
									id="InputUnit"
									value="{servicesAndProducts>Unit}"
									showClearIcon="true"
									required="true"
									showSuggestion="true"
									suggestionItems="{
										path: '/HT006Set',
										templateShareable: false
									}"
								>
									<suggestionItems>
										<core:ListItem
											key="{Msehi}"
											text="{Msehi}"
											additionalText="{Msehl}"
										/>
									</suggestionItems>
								</Input>
								<Input
									value="{servicesAndProducts>Value}"
									type="Number"
									showClearIcon="true"
									change="_onChangeQuantity"
									required="true"
								/>
								<Input
									value="{servicesAndProducts>Total}"
									type="Number"
									showClearIcon="true"
									change="_onChangeTotal"
									required="true"
								/>
								<!-- <Input value="{
														parts: [{
															path: 'servicesAndProducts>Value'
														}],
														type: 'sap.ui.model.type.Currency',
														formatOptions: {
															showMeasure: false
														}
													}"/> -->
								<!-- <ObjectNumber number="{servicesAndProducts>Total}"/> -->
							</cells>
						</ColumnListItem>
					</items>
				</Table>
					<OverflowToolbar>
						<Button
							icon="{orderView>/messageIcon}"
							text="{=${message>/}.length}"
							visible="{=${message>/}.length > 0}"
							type="Emphasized"
							press="onMessagePopoverPress"
						/>
						<Button
							text="Clear"
							press="onClearMessages"
							visible="{=${message>/}.length > 0}"
						/>
						<ToolbarSpacer/>
						<ToolbarSpacer/>
						<Button
							text="{i18n>save}"
							type="Accept"
							press="_saveOrder"
						/>
						<Button
							text="{i18n>clear}"
							type="Reject"
							press="_clearOrderTable"
						/>
					</OverflowToolbar>
			</f:content>
		</f:SimpleForm>
        
	</Dialog>
</c:FragmentDefinition>